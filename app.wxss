/**app.wxss**/
@import "/pages/style/iconfont.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

image {
  width: 100%;
  height: 100%;
}

button {
  line-height: 2;
  font-size: 15px;
  width: 120px !important;
  font-weight: 400  !important;
}

button[type='primary'] {
  background-color: #f2784b !important;
}
